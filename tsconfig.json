        "rootDir": ".",
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": [
        "service.ts",
        "lib/**/*.ts",
        "lib/**/*.d.ts"
    ]
}
